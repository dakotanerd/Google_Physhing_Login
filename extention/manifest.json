{
  "manifest_version": 2,
  "name": "Cookie Extension",
  "version": "1.0",
  "description": "View and manage cookies",
  "permissions": [
    "cookies",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html"
  }
}
